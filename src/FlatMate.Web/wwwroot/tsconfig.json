{
  "compilerOptions": {
    "allowJs": true,
    "lib": [ "dom", "es5", "scripthost", "es2015" ],
    "noEmitHelpers": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true,
    "strictNullChecks": true,
    "target": "ES5"
  }
}