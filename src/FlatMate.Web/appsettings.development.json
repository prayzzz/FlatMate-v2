{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=flatmate;Integrated Security=true"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Trace"
      },
      {
        "Name": "LiterateConsole",
        "args": { "restrictedToMinimumLevel": "Information" }
      },
      {
        "Name": "Elasticsearch",
        "args": {
          "autoRegisterTemplate": true,
          "nodeUris": "http://192.168.1.101:9200",
          "indexFormat": "dev-flatmate-{0:yyyy.MM}",
          "inlineFields":  true
        }
      }
    ]
  }
}
