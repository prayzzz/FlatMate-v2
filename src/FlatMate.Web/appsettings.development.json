{
  "ConnectionStrings": {
    "Flatmate": {
      "Url": "Server=localhost\\sqlexpress;Database=flatmate;",
      "User": "flatmate",
      "Password": "flatmate"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithDemystifiedStackTraces"
    ],
    "WriteTo": [
      {
        "Name": "LiterateConsole",
        "args": {
          "restrictedToMinimumLevel": "Information"
        }
      }
    ]
  }
}