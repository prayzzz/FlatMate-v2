{
  "ConnectionStrings": {
    "Flatmate": {
      "Url": "Server=192.168.1.108;Database=flatmate;MultipleActiveResultSets=True",
      "User": "",
      "Password": ""
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithDemystifiedStackTraces"
    ],
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "../log/flatmate_{Date}.txt",
          "retainedFileCountLimit": 7
        }
      },
      {
        "Name": "LiterateConsole"
      },
      {
        "Name": "Elasticsearch",
        "args": {
          "templateName": "app-logs",
          "nodeUris": "http://192.168.1.101:9200",
          "indexFormat": "app-flatmate-{0:yyyy.MM}"
        }
      }
    ]
  }
}