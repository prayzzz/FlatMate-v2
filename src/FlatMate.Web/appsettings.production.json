{
  "ConnectionStrings": {
    "Production": "Server=localhost;Database=flatmate2;"
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": { "pathFormat": "../logs/flatmate.txt" }
      },
      { "Name": "LiterateConsole" }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}