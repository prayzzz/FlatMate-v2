{
  "ConnectionStrings": {
    "Production": "Server=localhost;Database=flatmate;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "../log/flatmate_{Date}.txt",
          "retainedFileCountLimit": 7
        }
      },
      {
        "Name": "LiterateConsole"
      },
      {
        "Name": "Elasticsearch",
        "args": {
          "autoRegisterTemplate": true,
          "nodeUris": "http://192.168.1.101:9200",
          "indexFormat": "app-flatmate-{0:yyyy.MM}"
        }
      }
    ]
  }
}